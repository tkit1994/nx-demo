{
  "version": 1,
  "projects": {
    "wow": {
      "root": "apps/wow",
      "sourceRoot": "apps/wow/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/wow",
            "index": "apps/wow/src/index.html",
            "main": "apps/wow/src/main.tsx",
            "polyfills": "apps/wow/src/polyfills.ts",
            "tsConfig": "apps/wow/tsconfig.app.json",
            "assets": [
              "apps/wow/src/favicon.ico",
              "apps/wow/src/assets"
            ],
            "styles": [
              "apps/wow/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/wow/src/environments/environment.ts",
                  "with": "apps/wow/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "wow:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "wow:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/wow/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/wow"
          ],
          "options": {
            "jestConfig": "apps/wow/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "nx-electron:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "package": {
          "builder": "nx-electron:package",
          "options": {
            "name": "api",
            "frontendProject": "wow",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "builder": "nx-electron:make",
          "options": {
            "name": "api",
            "frontendProject": "wow",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "wow"
}